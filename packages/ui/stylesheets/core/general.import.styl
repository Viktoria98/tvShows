*
  box-sizing border-box
body
  font-family 'Helvetica', 'Arial', sans-serif
  margin 0
  padding 0 40px
  font-size 14px
ul
  margin 0
  padding 0
li
  list-style-type none
a
  text-decoration none
dl
  width 100%
  overflow hidden
  padding 0
  margin 0
dt
  float left
  text-align left
  padding 0
  margin 2px 0
  clear right
  padding-right 20px
dd
  float right
  text-align right
  padding 0
  margin 2px 0
