{
  "name": "ui-elements",
  "version": "1.0.0",
  "description": "Frontend package containing ui elements for company projects",
  "main": "./dist/main.js",
  "scripts": {
    "start": "webpack --config webpack.config.js",
    "dev": "webpack --progress --watch --config webpack.config.js",
    "precommit": "lint-staged",
    "postinstall": "mkdir -p node_modules/react && rm -rf node_modules/react",
    "eslint": "LIST=`git diff-index --name-only origin/master | grep -E '.(js|jsx)$';`; eslint --ignore-pattern '!.eslintrc.js' $LIST --color"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --eslint-config-path ./.eslintrc.js --single-quote --bracket-spacing --trailing-comma es5 --write",
      "git add"
    ]
  },
  "author": "Stanislav Tyshchuk",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "immutability-helper": "^2.4.0",
    "json-loader": "^0.5.4",
    "lint-staged": "^6.1.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-autosuggest": "9.3.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "anchorme": "^1.1.2",
    "babel-preset-stage-2": "^6.22.0",
    "classnames": "^2.2.6",
    "color-parser": "^0.1.0",
    "copy-to-clipboard": "^3.0.5",
    "d3": "^3.5.17",
    "decorate-component-with-props": "1.0.2",
    "easy-color": "^1.0.4",
    "ff-react-daterange-picker": "^1.0.1",
    "find-with-regex": "1.0.2",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "moment-range": "^3.1.1",
    "moment-timezone": "^0.5.11",
    "numeral": "^2.0.6",
    "prop-types": "15.6.0",
    "react": "^15.0.2",
    "react-day-picker": "^7.0.7",
    "react-dnd": "2.4.0",
    "react-dnd-html5-backend": "2.4.1",
    "react-dropzone": "^4.2.8",
    "react-dropzone-component": "^3.1.2"
  }
}
