@font-face
  font-family 'gotham-medium'
  src url("/packages/ff_ui-elements/assets/fonts/gotham-medium/gotham-medium-webfont.eot")
  src url("/packages/ff_ui-elements/assets/fonts/gotham-medium/gotham-medium-webfont.eot?#iefix") format("embedded-opentype"),
      url("/packages/ff_ui-elements/assets/fonts/gotham-medium/gotham-medium-webfont.woff2") format("woff2"),
      url("/packages/ff_ui-elements/assets/fonts/gotham-medium/gotham-medium-webfont.woff") format("woff"),
      url("/packages/ff_ui-elements/assets/fonts/gotham-medium/gotham-medium-webfont.ttf") format("truetype"),
      url("/packages/ff_ui-elements/assets/fonts/gotham-medium/gotham-medium-webfont.svg#gotham_mediumregular") format("svg")
  font-weight normal
  font-style normal

